{"ast":null,"code":"import \"core-js/modules/es.array.includes.js\";\nexport default {\n  name: 'TableComp',\n  props: ['dataset', 'getdata'],\n\n  data() {\n    return {\n      selected_data: {},\n      searchQuery: '',\n      searchkey: [],\n      columns: ['Title', 'Organizations', 'URL'],\n      target: String,\n      source: String,\n      keyword_data: [],\n      data: this.dataset,\n      color: {}\n    };\n  },\n\n  computed: {\n    filtered() {\n      if (this.searchQuery.length > 1) {\n        return this.data.filter(item => {\n          var columns = item.Title + item.Organizations + item.UNIQUEID + item.URL + item.Keyword.split(',');\n          return this.searchQuery.toLowerCase().split(' ').every(v => columns.toLowerCase().includes(v));\n        });\n      } else if (this.searchkey.length >= 1) {\n        return this.data.filter(item => {\n          return item.Keyword.split(',').includes(this.searchkey[0]);\n        });\n      } else {\n        return this.data;\n      }\n    }\n\n  },\n  methods: {\n    handleSelectionChange(val) {\n      this.selected_data = val;\n    },\n\n    handleSource(index) {\n      this.source = this.columns[index - 1];\n    },\n\n    handleTarget(index) {\n      this.target = this.columns[index - 1];\n    }\n\n  }\n};","map":{"version":3,"mappings":";AAyEA,eAAc;EACVA,IAAI,EAAC,WADK;EAEVC,KAAK,EAAC,CAAC,SAAD,EAAW,SAAX,CAFI;;EAGVC,IAAI,GAAE;IACF,OAAO;MACPC,aAAa,EAAC,EADP;MAEPC,WAAW,EAAC,EAFL;MAGPC,SAAS,EAAC,EAHH;MAIPC,OAAO,EAAC,CAAC,OAAD,EAAS,eAAT,EAAyB,KAAzB,CAJD;MAKPC,MAAM,EAACC,MALA;MAMPC,MAAM,EAACD,MANA;MAOPE,YAAY,EAAC,EAPN;MAQPR,IAAI,EAAC,KAAKS,OARH;MASPC,KAAK,EAAC;IATC,CAAP;EAWH,CAfS;;EAgBVC,QAAQ,EAAE;IACNC,QAAQ,GAAE;MACN,IAAG,KAAKV,WAAL,CAAiBW,MAAjB,GAA0B,CAA7B,EAA+B;QAC3B,OAAO,KAAKb,IAAL,CAAUc,MAAV,CAAkBC,IAAD,IAAQ;UAAE,IAAIX,OAAM,GAAIW,IAAI,CAACC,KAAL,GAAaD,IAAI,CAACE,aAAlB,GAAkCF,IAAI,CAACG,QAAvC,GAAkDH,IAAI,CAACI,GAAvD,GAA2DJ,IAAI,CAACK,OAAL,CAAaC,KAAb,CAAmB,GAAnB,CAAzE;UAClC,OAAO,KAAKnB,WAAL,CAAiBoB,WAAjB,GAA+BD,K